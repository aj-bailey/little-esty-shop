<%= render partial: 'partials/admin_invoice_nav_bar' %>

<div class="body_container">
  <div class="sub_container">
    <div class="sub_header">Invoices</div>
    <% @invoices.each do |invoice| %>
      <div id="invoice-<%= invoice.id %>">
        <p>
          <%= "Invoice -"%>
          <%= link_to "##{invoice.id}", admin_invoice_path(invoice) %>
        </p>
      </div>
    <% end %>
  </div>
</div>